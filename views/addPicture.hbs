<html lang='en'>
  <head>
    <meta charset='utf-8' />
    <meta
      name='viewport'
      content='width=device-width, initial-scale=1, shrink-to-fit=no'
    />
    <meta name='author' content='Untree.co' />
    <link rel='shortcut icon' href='/favicon.png' />

    <meta name='description' content='' />
    <meta name='keywords' content='free template, bootstrap, bootstrap4' />
    {{>link}}
    <title>모찌랑 만두랑</title>
  </head>
  <body>
    <div class='lines-wrap'>
      <div class='lines-inner'>
        <div class='lines'></div>
      </div>
    </div>
    <!-- END lines -->
    <!-- Nav Bar -->
    {{>header}}
    <!-- Nav Bar End-->
    <div class='custom-hero'>
      <div class='container'>
        <div class='row justify-content-center'>
          <div class='col-lg-6'>
            <h1 class='hero-heading'>Gallery</h1>
            <div class='line js-line mx-auto mb-3 my-3'></div>
            <p class='sub-text'>Memories of you and me</p>
          </div>
        </div>
      </div>
    </div>
    <!-- .accordion-item -->
    <div class='container pb-5 mb-5 border-bottom'>
      <div class='login-box'>
        <form id='writeD'>
          <div class='form-group'>
            <label for='date'>DATE</label>
            <input type='date' id='date' name='date' class='form-control' /><br
            /><br />
            <label class='text-black' for='file'>FILE</label>
            <input type='file' class='form-control' id='FILE' />
          </div>
        </form>
        <button type='submit' class='btn btn-primary' onclick='writeDiary()'>Add
          Picture</button>
      </div>
    </div>
    <!-- Loding -->
    {{>loding}}
    <!-- Loding End -->
    {{>script}}
    <script src='./js2/addPicture.js'></script>
    <script src='./js2/checkToken.js'></script>
